.nav{
  width: 100%;
  overflow: hidden;
  position: relative;
  top: 0;
  left: 0;
  z-index: 10;
}
.nav-title{
  width: 100%;
  height: 90rpx;
  line-height: 90rpx;
  text-align: center;
  position: absolute;
  bottom: 0;
  left: 0;
  z-index: 10;
  font-size:34rpx;
  letter-spacing:2px;
}
.nav .back{
  width: 50rpx;
  height: 50rpx;
  position: absolute;
  bottom: 0;
  left: 0;
  padding: 20rpx 30rpx;
}
.bg-white{
  background-color: #ffffff;
}
.bg-gray{
  background-color: #f7f7f7;
}
.overflow{
  overflow: auto;
}
.hidden{
  overflow: hidden;
}